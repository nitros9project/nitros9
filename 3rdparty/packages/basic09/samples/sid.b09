PROCEDURE sid
(* SID Sound test by: Roger Taylor *)
DIM rr,pp,page:INTEGER
page=$C4
pp=$00
RUN wild("MapBlk",page,rr)
m=rr+pp
PRINT "Mapped in="; rr
FOR Z=0 TO 23
POKE m+Z,0
NEXT Z
POKE m+24,15
POKE m+5,168
POKE m+6,169
POKE m+12,88
POKE m+13,89
POKE m+19,88
POKE m+20,89
100 REM LOOP
READ a,b,c,d,e,f,g
IF a<0 THEN 2000
POKE m+1,a
POKE m+0,b
POKE m+8,c
POKE m+7,d
POKE m+15,e
POKE m+14,f
POKE m+4,17
POKE m+11,33
POKE m+18,65
POKE m+16,128
POKE m+17,0
FOR Z=1 TO g
NEXT Z
GOTO 100
2000 POKE m+24,0
RUN wild("ClrBlk",page,r)
END 
DATA 15,210,0,0,0,0,2000
DATA 15,210,21,31,0,0,2000
DATA 15,210,21,31,23,181,2000
DATA 28,49,21,31,23,181,2000
DATA 28,49,37,162,23,181,2000
DATA 28,49,37,162,42,62,2000
DATA 28,49,21,31,31,165,2000
DATA -1,-1,-1,-1,-1,-1,-1
