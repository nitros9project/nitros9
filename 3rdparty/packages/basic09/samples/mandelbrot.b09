PROCEDURE mandelbrotRUN foenix("Clear")RUN foenix("CurOff")DIM i,px,py:INTEGERDIM x,y,xz,yz,xt:REALDIM bmblock:INTEGERRUN foenix("Bitmap",0,0,bmblock,0,0,"/DD/CMDS/XTCLUT")RUN foenix("Gon")RUN foenix("BMClear",bmblock,2)FOR py=0 TO 239FOR px=0 TO 319xz=px*3.5/319.-2.5yz=py*2./239.-1x=.0y=.0FOR i=0 TO 14IF x*x+y*y>4 THEN GOTO 230ENDIF xt=x*x-y*y+xzy=2*x*y+yzx=xtNEXT i230 RUN foenix("Pixel",px,py,i,bmblock)NEXT pxNEXT pyRUN foenix("CurOn")END 