PROCEDURE circle
DIM name:STRING
DIM bm0:INTEGER
FOR i=$FEE0 TO $FEEF
POKE i,$00
NEXT i
RUN WILD("Clear")
RUN WILD("Bitmap",0,0,bm0,0,0,"/DD/CMDS/XTCLUT")
RUN WILD("Gon")
RUN WILD("BMClear",bm0,0)
RUN WILD("Circle",100,100,15,0,6,bm0)
FOR i=$FEE0 TO $FEEF
PRINT PEEK(i); "  "; 
NEXT i
INPUT "Cont <Enter>",name
RUN WILD("BMoff",0)
RUN WILD("Goff")
