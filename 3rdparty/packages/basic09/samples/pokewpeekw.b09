PROCEDURE pokewpeekw(* Peekw and Pokew - 16 bit Word *)DIM value,value2:INTEGERvalue=10value2=11PRINT "Address of Value "; ADDR(value)PRINT "Address of Value2 "; ADDR(value2)RUN foenix("PokeW",ADDR(value),value)RUN foenix("PeekW",ADDR(value),value2)PRINT "Value="; valuePRINT "Size",SIZE(value)PRINT "Value2="; value2PRINT "Size Value2",SIZE(value2)PRINT PEEK(ADDR(value))*256+PEEK(ADDR(value)+1)PRINT PEEK(ADDR(value2))*256+PEEK(ADDR(value2)+1)