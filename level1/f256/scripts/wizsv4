echo * WizFi360 Helper - Start packet server
xmode /wz eko=0
echo ATE0>/wz
modem /wz /wz
echo AT+CIPMODE=0>/wz
modem /wz /wz
echo AT+CIPMUX=1>/wz
modem /wz /wz
echo AT+CIPSERVERMAXCONN=4>/wz
modem /wz /wz
echo AT+CIPSERVER=1,23>/wz
modem /wz /wz
echo AT+CIPSTO=0>/wz
modem /wz /wz
modem /wz
echo * Done
